        <script><![CDATA[function onSubmit() {
	
	var type = g_form.getValue('request_type');
	var wfs = g_form.getValue('wfs');
//Works in non-portal ui
	if(type == 'Add New Dealer User')
		{
			try {
				var attachments = document.getElementById('header_attachment_list_label');
				if (attachments.style.visibility == 'hidden' || attachments.style.display == 'none' ) {
					if(wfs == 'true')
						{
							alert('You must attach Finance Portal Request Form and Dealer Setup Form before submitting this request.');
							return false;
						}else{
							alert('You must attach Finance Portal Request Form before submitting this request.');
							return false;
						}
					
				}
			}
//For Service Portal
			catch(e) {
				var count = getSCAttachmentCount();
				if(count <= 0) {
					if(wfs == 'true')
						{
							alert('You must attach Finance Portal Request Form and Dealer Setup Form before submitting this request.');
							return false;
						}else{
							alert('You must attach Finance Portal Request Form before submitting this request.');
							return false;
						}
				}
			}
		}
}]]></script>
